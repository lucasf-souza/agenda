<% if (errors && errors.length > 0) { %>
  <div class="alert alert-danger">
    <ul>
      <% errors.forEach(function(error) { %>
        <li><%= error %></li>
      <% }); %>
    </ul>
  </div>
<% } %>

<% if (success && success.length > 0) { %>
  <div class="alert alert-success">
    <ul>
      <% success.forEach(function(msg) { %>
        <li><%= msg %></li>
      <% }); %>
    </ul>
  </div>
<% } %>
